! Base16 Railscasts
! Scheme: Ryan Bates (http://railscasts.com)

#define base00 #2b2b2b
#define base01 #272935
#define base02 #3a4055
#define base03 #5a647e
#define base04 #d4cfc9
#define base05 #e6e1dc
#define base06 #f4f1ed
#define base07 #f9f7f3
#define base08 #da4939
#define base09 #cc7833
#define base0A #ffc66d
#define base0B #a5c261
#define base0C #519f50
#define base0D #6d9cbe
#define base0E #b6b3eb
#define base0F #bc9458

*.foreground:   base05
*.background:   base00
*.cursorColor:  base05

*.color0:       base00
*.color1:       base08
*.color2:       base0B
*.color3:       base0A
*.color4:       base0D
*.color5:       base0E
*.color6:       base0C
*.color7:       base05

*.color8:       base03
*.color9:       base08
*.color10:      base0B
*.color11:      base0A
*.color12:      base0D
*.color13:      base0E
*.color14:      base0C
*.color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*.color16:      base09
*.color17:      base0F
*.color18:      base01
*.color19:      base02
*.color20:      base04
*.color21:      base06


URxvt.intensityStyles: false
URxvt*cursorBlink:     true
URxvt*font:            xft:DejaVu Sans Mono for Powerline:size=11
URxvt*boldFont:        xft:DejaVu Sans Mono for Powerline:size=11
URxvt*scrollBar:       false
URxvt.perl-ext-common: default,matcher,font-size
URxvt.url-launcher:    chromium
URxvt.matcher.button:  1
URxvt.internalBorder:  2
URxvt.lineSpace:       0
URxvt.letterSpace:     -2
URxvt.iso14755: False
URxvt.geometry: 130x35

rofi.width:           100
rofi.lines:           6
rofi.opacity:         95
rofi.font:            DejaVu Sans Mono 13
rofi.terminal:        rxvt-unicode
rofi.padding:         600
rofi.bw:              0

! State:           bg             fg             bgalt          hlbg           hlfg
! rofi.color-normal: argb:22002b36, argb:aa839496, argb:22002b36, argb:77002b36, argb:ff839496
! rofi.color-window: argb:00000000, argb:00000000
! rofi.columns:         1
! rofi.loc:             0
! rofi.yoffset:         0
! rofi.xoffset:         0
! rofi.fixed-num-lines: false
! rofi.hmode:           false
! rofi.ssh-set-title:   true
