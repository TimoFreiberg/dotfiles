abbr -a -U -- Au 'sudo aura -Au'
abbr -a -U -- :q exit
abbr -a -U -- abbred 'vim ~/.config/fish/abbreviations; and clearabbr; and source $HOME/.config/fish/abbreviations'
abbr -a -U -- abbrw 'abbr > ~/.config/fish/abbreviations'
abbr -a -U -- cal 'cal -3 -w -m'
abbr -a -U -- g git
abbr -a -U -- getabbr 'clearabbr; and source $HOME/.config/fish/abbreviations'
abbr -a -U -- gpl 'git pull'
abbr -a -U -- gs 'git ss'
abbr -a -U -- l exa -l
abbr -a -U -- l1 ls-1A
abbr -a -U -- la exa -la
abbr -a -U -- ll 'ls -A'
abbr -a -U -- mnt 'udevil mount'
abbr -a -U -- parvim 'parallel -X --tty vim'
abbr -a -U -- pm pacman
abbr -a -U -- sc systemctl
abbr -a -U -- scX 'sudo systemctl start'
abbr -a -U -- scr 'sudo systemctl restart'
abbr -a -U -- scx 'sudo systemctl stop'
abbr -a -U -- spr 'sudo pacman -R'
abbr -a -U -- sps 'sudo pacman -S'
abbr -a -U -- ssc 'sudo systemctl'
abbr -a -U -- syu 'sudo pacman -Syu'
abbr -a -U -- t trash
abbr -a -U -- tp trash-put
